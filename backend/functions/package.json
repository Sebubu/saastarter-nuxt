{
  "name": "functions",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "build": "tsc",
    "build-watch": "tsc --watch",
    "emulate": "firebase emulators:start --inspect-functions --import=./emulator_data --export-on-exit",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy",
    "logs": "firebase functions:log",
    "dev": "concurrently \"npm run build-watch\" \"npm run emulate\"",
    "test": "firebase emulators:exec 'jest --verbose=false --detectOpenHandles' --only firestore"
  },
  "engines": {
    "node": "10"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@firebase/testing": "^0.20.11",
    "express": "^4.17.1",
    "firebase-admin": "^8.10.0",
    "firebase-functions": "^3.6.1",
    "saastarter-fire": "^1.0.0-beta4"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.13",
    "firebase-functions-test": "^0.2.0",
    "jest": "^26.4.2",
    "ts-jest": "^26.3.0",
    "tslint": "^5.12.0",
    "typescript": "^3.8.0"
  },
  "private": true
}
